        android:supportsRtl="true"
        android:theme="@style/Theme.EMICalculator">
        <activity android:name=".LoanCompairActivity"
            android:parentActivityName=".MainActivity"
            />
        <activity android:name=".interestpayout.InterestStatisticsActivity"
            android:parentActivityName=".fdcalculator.InterestPayoutActivity"
